{
    "name": "art-craft/yii2-app-advanced",
    "description": "Custom Semantic URLs, admin panel, user, debug, sass",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://web-artcraft.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/apuc/yii2-advanced"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "dektrium/yii2-user": "0.9.*@dev",
        "dektrium/yii2-rbac": "dev-master",
        "2amigos/yii2-transliterator-helper": "*",
        "zelenin/yii2-rss": "~0.1",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-widget-colorinput": "*",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "kartik-v/bootstrap-fileinput": "dev-master",
        "yiisoft/yii2-imagine": "~2.0.0",
        "himiklab/yii2-sitemap-module" : "*",
        "himiklab/yii2-ipgeobase-component": "*",
        "kartik-v/yii2-export": "*",
        "miloschuman/yii2-highcharts-widget": "*",
        "cagartner/phpquery": "*",
        "abraham/twitteroauth": "^0.7.4",
        "kartik-v/bootstrap-star-rating": "@dev"

    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "mihaildev/yii2-ckeditor": "*",
        "mihaildev/yii2-elfinder": "*",
        "vision/yii2-private-messages": "dev-master",
        "zelenin/yii2-rss": "~0.1"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
